<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <servlet>
        <servlet-name>anyName</servlet-name>
        <servlet-class>com.GanHaoyu.week2.demo.Helloword</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>LifecycleServlet</servlet-name>
        <servlet-class>com.week3.demo.LifecycleServlet</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>RegisterServlet1</servlet-name>
        <servlet-class>com.week3.demo.RegisterServlet1</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>anyName</servlet-name>
        <url-pattern>/hello</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>LifecycleServlet</servlet-name>
        <url-pattern>/life</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>RegisterServlet1</servlet-name>
        <url-pattern>/register</url-pattern>
    </servlet-mapping>
</web-app>